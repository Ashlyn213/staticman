{
  "version": 2,
  "builds": [
    {
      "src": "api/staticman.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/v3/entry/(.*)",
      "dest": "/api/staticman.js"
    }
  ]
}
{
  "redirects": [
    {
      "source": "/v3/entry/Ashlyn213/my_story/main",
      "destination": "/v3/entry/Ashlyn213/my_story/main",
      "permanent": false
    }
  ]
}
